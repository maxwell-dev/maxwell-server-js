{
  "name": "maxwell-server-js",
  "version": "0.6.0",
  "description": "Maxwell server implementation for js/ts.",
  "author": "Chaoqian Xu <chaoranxu@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "pack": "npx webpack --config webpack.config.js --progress",
    "serve": "npm run pack && node dist/index.bundle.js",
    "test": "npx jest --coverage",
    "format": "npx prettier --write \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\" --ignore-path *.d.ts",
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern *.d.ts",
    "clean": "bin/clean.sh"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "repository": "https://github.com/maxwell-dev/maxwell-server-js.git",
  "dependencies": {
    "lru-cache": "^10.0.0",
    "maxwell-protocol": "https://github.com/maxwell-dev/maxwell-protocol-js.git#v1.1.1",
    "maxwell-utils": "https://github.com/maxwell-dev/maxwell-utils-js.git#v0.6.0",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.5",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@msgpack/msgpack": "2.8.0",
    "@types/ws": "8.5.5",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "babel-jest": "29.5.0",
    "bufferutil": "4.0.7",
    "canvas": "2.11.2",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "utf-8-validate": "6.0.3",
    "webpack": "5.81.0",
    "webpack-cli": "5.0.2"
  },
  "peerDependencies": {}
}
